    modelName="Slam" codeFormatting="true" commentFormatting="true" testsDirectory="/org.ietr.dftools.architecture.tests/ecore-gen"
    importerID="org.eclipse.emf.importer.ecore" complianceLevel="8.0" copyrightFields="false"
    importOrganizing="true" cleanup="true">
  <foreignModel>Slam.ecore</foreignModel>
  <genPackages prefix="Slam" basePackage="org.ietr.dftools.architecture" disposableProviderFactory="true"
      ecorePackage="Slam.ecore#/">
    <genClasses ecoreClass="Slam.ecore#//Design">
      <genFeatures property="None" children="true" createChild="true" ecoreFeature="ecore:EReference Slam.ecore#//Design/componentInstances"/>
      <genFeatures property="None" children="true" createChild="true" ecoreFeature="ecore:EReference Slam.ecore#//Design/links"/>
      <genFeatures property="None" children="true" createChild="true" ecoreFeature="ecore:EReference Slam.ecore#//Design/hierarchyPorts"/>
      <genFeatures property="None" notify="false" createChild="false" ecoreFeature="ecore:EReference Slam.ecore#//Design/refined"/>
      <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//Design/path"/>
      <genFeatures notify="false" createChild="false" propertySortChoices="true" ecoreFeature="ecore:EReference Slam.ecore#//Design/componentHolder"/>
      <genOperations ecoreOperation="Slam.ecore#//Design/containsComponentInstance">
        <genParameters ecoreParameter="Slam.ecore#//Design/containsComponentInstance/name"/>
      </genOperations>
      <genOperations ecoreOperation="Slam.ecore#//Design/containsComponent">
        <genParameters ecoreParameter="Slam.ecore#//Design/containsComponent/name"/>
      </genOperations>
      <genOperations ecoreOperation="Slam.ecore#//Design/getComponentInstance">
        <genParameters ecoreParameter="Slam.ecore#//Design/getComponentInstance/name"/>
      </genOperations>
      <genOperations ecoreOperation="Slam.ecore#//Design/getComponent">
        <genParameters ecoreParameter="Slam.ecore#//Design/getComponent/name"/>
        <genParameters ecoreParameter="Slam.ecore#//Design/getComponent/class"/>
      </genOperations>
    </genClasses>
    <genClasses ecoreClass="Slam.ecore#//ComponentInstance">
      <genFeatures notify="false" createChild="false" propertySortChoices="true" ecoreFeature="ecore:EReference Slam.ecore#//ComponentInstance/component"/>
      <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//ComponentInstance/instanceName"/>
      <genOperations ecoreOperation="Slam.ecore#//ComponentInstance/isHierarchical"/>
    </genClasses>
    <genClasses ecoreClass="Slam.ecore#//VLNVedElement">
      <genFeatures property="None" children="true" createChild="true" ecoreFeature="ecore:EReference Slam.ecore#//VLNVedElement/vlnv"/>
    </genClasses>
    <genClasses ecoreClass="Slam.ecore#//ParameterizedElement">
      <genFeatures property="None" children="true" createChild="true" ecoreFeature="ecore:EReference Slam.ecore#//ParameterizedElement/parameters"/>
    </genClasses>
    <genClasses ecoreClass="Slam.ecore#//ComponentHolder">
      <genFeatures property="None" children="true" createChild="true" ecoreFeature="ecore:EReference Slam.ecore#//ComponentHolder/components"/>
    </genClasses>
