        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//component/ComNode/parallel"/>
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//component/ComNode/speed"/>
      </genClasses>
      <genClasses image="false" ecoreClass="Slam.ecore#//component/Enabler"/>
      <genClasses ecoreClass="Slam.ecore#//component/Dma">
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//component/Dma/setupTime"/>
      </genClasses>
      <genClasses ecoreClass="Slam.ecore#//component/Mem">
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//component/Mem/size"/>
      </genClasses>
      <genClasses ecoreClass="Slam.ecore#//component/HierarchyPort">
        <genFeatures notify="false" createChild="false" propertySortChoices="true"
            ecoreFeature="ecore:EReference Slam.ecore#//component/HierarchyPort/externalInterface"/>
        <genFeatures notify="false" createChild="false" propertySortChoices="true"
            ecoreFeature="ecore:EReference Slam.ecore#//component/HierarchyPort/internalInterface"/>
        <genFeatures notify="false" createChild="false" propertySortChoices="true"
            ecoreFeature="ecore:EReference Slam.ecore#//component/HierarchyPort/internalComponentInstance"/>
      </genClasses>
      <genClasses ecoreClass="Slam.ecore#//component/ComInterface">
        <genFeatures property="None" notify="false" createChild="false" ecoreFeature="ecore:EReference Slam.ecore#//component/ComInterface/component"/>
        <genFeatures property="None" children="true" createChild="true" ecoreFeature="ecore:EReference Slam.ecore#//component/ComInterface/busType"/>
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//component/ComInterface/name"/>
      </genClasses>
    </nestedGenPackages>
    <nestedGenPackages prefix="Link" disposableProviderFactory="true" ecorePackage="Slam.ecore#//link">
      <genClasses image="false" ecoreClass="Slam.ecore#//link/Link">
        <genFeatures notify="false" createChild="false" propertySortChoices="true"
            ecoreFeature="ecore:EReference Slam.ecore#//link/Link/sourceInterface"/>
        <genFeatures notify="false" createChild="false" propertySortChoices="true"
            ecoreFeature="ecore:EReference Slam.ecore#//link/Link/destinationInterface"/>
        <genFeatures notify="false" createChild="false" propertySortChoices="true"
            ecoreFeature="ecore:EReference Slam.ecore#//link/Link/sourceComponentInstance"/>
        <genFeatures notify="false" createChild="false" propertySortChoices="true"
            ecoreFeature="ecore:EReference Slam.ecore#//link/Link/destinationComponentInstance"/>
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//link/Link/uuid"/>
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//link/Link/directed"/>
      </genClasses>
      <genClasses ecoreClass="Slam.ecore#//link/DataLink"/>
      <genClasses ecoreClass="Slam.ecore#//link/ControlLink"/>
    </nestedGenPackages>
    <nestedGenPackages prefix="Attributes" disposableProviderFactory="true" ecorePackage="Slam.ecore#//attributes">
      <genClasses ecoreClass="Slam.ecore#//attributes/VLNV">
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//attributes/VLNV/vendor"/>
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//attributes/VLNV/library"/>
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//attributes/VLNV/name"/>
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//attributes/VLNV/version"/>
      </genClasses>
      <genClasses ecoreClass="Slam.ecore#//attributes/Parameter">
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//attributes/Parameter/key"/>
        <genFeatures createChild="false" ecoreFeature="ecore:EAttribute Slam.ecore#//attributes/Parameter/value"/>
      </genClasses>
    </nestedGenPackages>
  </genPackages>
